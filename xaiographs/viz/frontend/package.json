{
    "name": "XAIoWeb",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --disable-host-check --port 3000",
        "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && ng build",
